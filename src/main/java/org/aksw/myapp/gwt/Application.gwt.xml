<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>

      <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->


    <!-- Other module inherits                                      -->
  <!-- Specify the app entry point class.                         -->
  <!--<entry-point class='org.example.myproject.client.GWTTest'/> -->
  <!--  <inherits name='org.krayne.gollum.Gollum'/> -->
  <!-- <inherits name='org.gwtopenmaps.openlayers.OpenLayers'/> -->
  <!-- <inherits name="com.smartgwt.SmartGwt"/>   -->
  <!--   <inherits name='com.extjs.gxt.ui.GXT'/> -->
  <!-- <inherits name='com.pietschy.gwt.pectin.Pectin'/>-->

      <!-- Specify the app entry point class.                         -->
      <entry-point class='org.aksw.myapp.gwt.client.Application'/>

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Application.css' />

    <!--
        this defines to where the js request is going to
        corresponds with the mapping in web.xml
    -->
      <servlet class="org.aksw.myapp.gwt.EntryPointServlet" path="/WorkerService"/>
      

      <set-property name="user.agent" value="gecko1_8"></set-property>
    <!-- <set-property name="locale" values="en_UK"></set-property> -->

    <!-- Specify the paths for translatable code                    -->
    <!-- <source path='client'/>
        <source path='shared'/>-->

</module>


